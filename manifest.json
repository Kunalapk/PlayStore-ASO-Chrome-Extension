{
  "manifest_version": 2,

  "name": "Playstore Analytics",
  "description": "This extension will help you with Playstore ASO",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["test.js"]
        }
      ],
  "permissions": [
    "activeTab"
  ]
}